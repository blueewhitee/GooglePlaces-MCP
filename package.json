{
  "name": "talkforce.ai",
  "version": "1.0.0",
  "description": "AI-powered location services providing both REST API and MCP server for place search, nearby discovery, and detailed place information",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "mcp": "node dist/mcp-server.js",
    "mcp:dev": "nodemon --exec \"npx tsx src/mcp-server.ts\"",
    "build:mcp": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "talkforce-mcp": "./dist/mcp-server.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "location",
    "places",
    "google-places",
    "search",
    "nearby"
  ],
  "author": "TalkForce.ai",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.14",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@googlemaps/places": "^2.1.0",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.19.2"
  }
}
